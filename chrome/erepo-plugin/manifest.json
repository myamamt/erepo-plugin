{
	"manifest_version": 2,
	"name": "Error Repository Plugin",
	"version": "1.0.0",

    "permissions": [
        "storage"
    ],

	"content_scripts": [{
		"matches": [ "<all_urls>" ],
    	"js": [ "script.js" ],
    	"run_at": "document_start"
	}],

    "icons": {
        "64": "icon.png"
    },

    "options_page": "options.html"
}
